{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "aimtable.v1.schema.json",
  "title": "AimTable v1",
  "type": "object",
  "additionalProperties": false,
  "required": ["version", "items"],
  "properties": {
    "version": { "const": "aimtable.v1" },
    "items": {
      "type": "array",
      "minItems": 9,
      "maxItems": 9,
      "items": { "$ref": "#/definitions/AimNode" }
    }
  },
  "definitions": {
    "AimNode": {
      "type": "object",
      "additionalProperties": false,
      "required": ["id", "label", "cluster"],
      "properties": {
        "id":  { "$ref": "types.common.v1.schema.json#/definitions/AimId" },
        "label": { "type": "string", "minLength": 1 },
        "cluster": { "$ref": "types.common.v1.schema.json#/definitions/Cluster" },
        "description": { "type": "string" }
      }
    }
  }
}
