{
  "$id": "https://hokusei.dev/schema/aimtable.v1.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AimTable v1",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "version": { "const": "aimtable.v1" },
    "items": {
      "type": "array",
      "minItems": 9,
      "items": {
        "type": "object",
        "additionalProperties": true,
        "properties": {
          "id": { "type": "string", "minLength": 1 },
          "label": { "type": "string" }
        },
        "required": ["id"]
      }
    }
  },
  "required": ["version", "items"]
}
